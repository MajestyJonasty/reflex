<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <PackageId>ReFlex.Core.Events</PackageId>
    <Authors>Mathias Müller</Authors>
    <Copyright>Copyright ©  2021-2023</Copyright>
    <Company>
      GTV - Gesellschaft fuer Technische Visualistik mbH, Dresden
      Hochschule fuer Technik und Wirtschaft Dresden, Dresden
    </Company>
    <Version>1.0.0</Version>
    <Product>ReFlex.Core</Product>
    <AssemblyName>ReFlex.Core.Events</AssemblyName>
    <RootNamespace>ReFlex.Core.Events</RootNamespace>
    <Platforms>x64</Platforms>
    <IncludeNativeLibrariesForSelfExtract>true</IncludeNativeLibrariesForSelfExtract>
    <Configurations>Debug;Release;CI;Debug OSX;Release OSX</Configurations>
  </PropertyGroup>

  <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
    <PlatformTarget>x64</PlatformTarget>
  </PropertyGroup>

  <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
    <PlatformTarget>x64</PlatformTarget>
  </PropertyGroup>

  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug OSX|x64' ">
    <PlatformTarget>x64</PlatformTarget>
    <DebugSymbols Condition=" '$(DebugSymbols)' == '' ">true</DebugSymbols>
    <Optimize Condition=" '$(Optimize)' == '' ">false</Optimize>
  </PropertyGroup>

  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release OSX|x64' ">
    <PlatformTarget>x64</PlatformTarget>
    <Optimize Condition=" '$(Optimize)' == '' ">true</Optimize>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Prism.Core" Version="8.1.97" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Tracking\Tracking.csproj" />
  </ItemGroup>

</Project>
